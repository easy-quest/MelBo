#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ccc/Projects/Python/MelBo/node_modules/.pnpm/pyright@1.1.194/node_modules/pyright/node_modules:/home/ccc/Projects/Python/MelBo/node_modules/.pnpm/pyright@1.1.194/node_modules:/home/ccc/Projects/Python/MelBo/node_modules/.pnpm/node_modules:/home/ccc/Projects/Python/MelBo/node_modules:/home/ccc/Projects/Python/node_modules:/home/ccc/Projects/node_modules:/home/ccc/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ccc/Projects/Python/MelBo/node_modules/.pnpm/pyright@1.1.194/node_modules/pyright/node_modules:/home/ccc/Projects/Python/MelBo/node_modules/.pnpm/pyright@1.1.194/node_modules:/home/ccc/Projects/Python/MelBo/node_modules/.pnpm/node_modules:/home/ccc/Projects/Python/MelBo/node_modules:/home/ccc/Projects/Python/node_modules:/home/ccc/Projects/node_modules:/home/ccc/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../langserver.index.js" "$@"
else
  exec node  "$basedir/../../langserver.index.js" "$@"
fi
